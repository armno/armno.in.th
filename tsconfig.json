{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "moduleResolution": "node",
    "strictNullChecks": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}